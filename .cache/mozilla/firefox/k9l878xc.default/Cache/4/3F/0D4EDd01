	$(document).ready(function() {
		$('#callout_trigger').callout({ flyout : '#callout' });
		$.getScript('/big-picture/_assets/scripts/big-picture.js', function(){
			$('#big-picture').bigpicture();
			$(document).everyTime(12000, function(i) {
				if ($('#big-picture-list ul li.active').next().is('li')) {
					$.bigpicture.activate($('#big-picture-list ul li.active').next());
				} else {
					$.bigpicture.activate($('#big-picture-list ul li').first());
				}
			}, 0);
		});
	});
	</script>
